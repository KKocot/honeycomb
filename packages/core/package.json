{
  "name": "@kkocot/honeycomb-core",
  "version": "0.1.0",
  "description": "Framework-agnostic core for Hive Blockchain connectivity with endpoint fallback",
  "keywords": [
    "hive",
    "blockchain",
    "web3",
    "honeycomb",
    "core"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KKocot/honeycomb"
  },
  "private": true,
  "license": "MIT",
  "author": "KKocot",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./src/styles.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/styles.css",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@hiveio/wax": ">=1.28.0"
  },
  "devDependencies": {
    "@hiveio/wax": "^1.28.4-rc0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
